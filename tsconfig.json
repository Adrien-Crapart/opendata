{
  "compilerOptions": {
    "target": "ES2018",
    "module": "ESNext",
    "moduleResolution": "Node",
    "lib": ["ESNext", "ESNext.AsyncIterable", "DOM"],
    "strict": true,
    "jsx": "preserve",
    "importHelpers": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "allowJs": true,
    "sourceMap": true,
    "verbatimModuleSyntax": true,
    "baseUrl": ".",
    "paths": {
      "#app": ["./.nuxt/app/index.mjs"],
      "#build": ["./.nuxt/build"],
      "#imports": ["./.nuxt/imports"],
      "#paths": ["./.nuxt/paths.mjs"],
      "#public": ["./public"],
      "#server": ["./server"],
      "#app/*": ["./.nuxt/app/*"],
      "#build/*": ["./.nuxt/build/*"],
      "#imports/*": ["./.nuxt/imports/*"],
      "#paths/*": ["./.nuxt/paths.mjs"],
      "#public/*": ["./public/*"],
      "#server/*": ["./server/*"],
      "~/*": ["./*"],
      "@/*": ["./*"]
    },
    "types": ["jest", "node"]
  },
  "exclude": ["node_modules", ".nuxt", "dist"],
  "include": ["**/*.ts", "**/*.tsx", "**/*.vue"],
  "extends": "./.nuxt/tsconfig.json"
}
